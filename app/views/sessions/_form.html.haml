= form_tag sessions_path do
  .field
    = label_tag :email
    = text_field_tag :email

  .field
    = label_tag :password
    = password_field_tag :password

  .actions
    = submit_tag "Sign in", class: "btn btn-large btn-primary"
= button_to "Create new account", new_user_path, class: "btn btn-success", :method => :get
